generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
}

model Company {
  id                 String             @id @default(uuid())
  name               String             @unique
  companyCode        String             @unique
  logo               String?
  address            String?
  phone              String?
  website            String?
  subscriptionStatus SubscriptionStatus @default(TRIAL)
  trialEndsAt        DateTime?
  subscriptionEndsAt DateTime?
  isActive           Boolean            @default(true)
  createdAt          DateTime           @default(now())
  updatedAt          DateTime           @updatedAt
  // NEW: Track if company name has been changed
  nameChangedAt      DateTime? // null means name can still be changed once

  activityLogs         ActivityLog[]
  departments          Department[]
  projects             Project[]
  sops                 Sop[]
  users                User[]
  screenCaptureEnabled Boolean       @default(true)

  @@map("companies")
}

model User {
  id                         String              @id @default(uuid())
  email                      String              @unique
  password                   String
  firstName                  String
  lastName                   String
  role                       UserRole            @default(USER)
  avatar                     String?
  phone                      String?
  isActive                   Boolean             @default(true)
  companyId                  String
  departmentId               String?
  startDate                  DateTime?
  endDate                    DateTime?
  points                     Int                 @default(0)
  createdAt                  DateTime            @default(now())
  updatedAt                  DateTime            @updatedAt
  activityLogs               ActivityLog[]
  chatMessages               ChatMessage[]
  chatRoomMembers            ChatRoomMember[]
  chatRoomsCreated           ChatRoom[]
  departmentProjectsAssigned DepartmentProject[] @relation("DepartmentProjectAssigner")
  departmentsLed             Department[]        @relation("DepartmentLead")
  notifications              Notification[]
  projectMemberships         ProjectMember[]
  projectsLed                Project[]
  sopsApproved               Sop[]               @relation("SopApprover")
  sopsCreated                Sop[]               @relation("SopCreator")
  subProjectsAssigned        SubProject[]        @relation("SubProjectAssignee")
  subProjectsCreated         SubProject[]        @relation("SubProjectCreator")
  // NEW: SubProject QC Head relation
  subProjectsAsQcHead        SubProject[]        @relation("SubProjectQcHead")
  // NEW: SubProject member relation
  subProjectMemberships      SubProjectMember[]
  // NEW: Task assignments
  tasksAssigned              Task[]              @relation("TaskAssignee")
  tasksCreated               Task[]              @relation("TaskCreator")
  timeTrackings              TimeTracking[]
  company                    Company             @relation(fields: [companyId], references: [id], onDelete: Cascade)
  department                 Department?         @relation(fields: [departmentId], references: [id])
  // Desktop agent tracking
  desktopAgents              DesktopAgent[]
  // NEW: User performance stats (denormalized for fast queries)
  totalTasksCompleted        Int                 @default(0)
  totalTimeTrackedMinutes    Int                 @default(0)
  totalPointsEarned          Int                 @default(0)
  currentStreak              Int                 @default(0) // Days in a row with completed tasks
  longestStreak              Int                 @default(0)
  lastActiveDate             DateTime?
  oneTimeCodes               OneTimeCode[]

  taskAssignments         TaskAssignee[] @relation("TaskAssignees")
  tasksAssignedByMe       TaskAssignee[] @relation("TaskAssigner")
  tasksSubmittedForReview Task[]         @relation("TaskSubmitter")
  tasksReviewed           Task[]         @relation("TaskReviewer")

  @@index([companyId])
  @@index([email])
  @@map("users")
}

model Department {
  id          String              @id @default(uuid())
  name        String
  description String?
  tag         String?
  logo        String? // NEW: Department logo URL
  companyId   String
  leadId      String?
  startDate   DateTime?
  endDate     DateTime?
  createdAt   DateTime            @default(now())
  updatedAt   DateTime            @updatedAt
  projects    DepartmentProject[]
  company     Company             @relation(fields: [companyId], references: [id], onDelete: Cascade)
  lead        User?               @relation("DepartmentLead", fields: [leadId], references: [id])
  users       User[]

  @@index([companyId])
  @@map("departments")
}

model DepartmentProject {
  id           String     @id @default(uuid())
  departmentId String
  projectId    String
  assignedAt   DateTime   @default(now())
  assignedById String?
  assignedBy   User?      @relation("DepartmentProjectAssigner", fields: [assignedById], references: [id])
  department   Department @relation(fields: [departmentId], references: [id], onDelete: Cascade)
  project      Project    @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@unique([departmentId, projectId])
  @@index([departmentId])
  @@index([projectId])
  @@map("department_projects")
}

model Project {
  id                      String              @id @default(uuid())
  name                    String
  description             String?
  budget                  Decimal?            @db.Decimal(10, 2)
  status                  ProjectStatus       @default(PLANNING)
  companyId               String
  projectLeadId           String?
  startDate               DateTime?
  endDate                 DateTime?
  // Screen capture settings - THIS IS THE KEY FIELD
  screenCaptureEnabled    Boolean             @default(false)
  screenCaptureInterval   Int                 @default(3) // Base interval in minutes (actual will be random 2-5)
  screenMonitoringEnabled Boolean             @default(false) // Legacy field, keeping for compatibility
  createdAt               DateTime            @default(now())
  updatedAt               DateTime            @updatedAt
  chatRooms               ChatRoom[]
  departments             DepartmentProject[]
  members                 ProjectMember[]
  company                 Company             @relation(fields: [companyId], references: [id], onDelete: Cascade)
  projectLead             User?               @relation(fields: [projectLeadId], references: [id])
  subProjects             SubProject[]

  @@index([companyId])
  @@map("projects")
}

model ProjectMember {
  id           String            @id @default(uuid())
  projectId    String
  userId       String
  role         ProjectMemberRole @default(MEMBER)
  pointsEarned Int               @default(0)
  joinedAt     DateTime          @default(now())
  project      Project           @relation(fields: [projectId], references: [id], onDelete: Cascade)
  user         User              @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([projectId, userId])
  @@index([projectId])
  @@index([userId])
  @@map("project_members")
}

model OneTimeCode {
  id        String    @id @default(uuid())
  code      String    @unique
  userId    String
  type      String // DESKTOP_AUTH, PASSWORD_RESET, etc.
  expiresAt DateTime
  usedAt    DateTime?
  metadata  Json?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([code])
  @@index([expiresAt])
  @@index([userId])
}

// ENHANCED: SubProject now has QC Head and members
model SubProject {
  id             String             @id @default(uuid())
  title          String
  description    String?
  projectId      String
  assignedToId   String? // Legacy - kept for backward compatibility
  createdById    String
  // NEW: QC Head for this subproject (must be QC_ADMIN role)
  qcHeadId       String?
  status         SubProjectStatus   @default(TODO)
  priority       Priority           @default(MEDIUM)
  pointsValue    Int                @default(0)
  estimatedHours Int?
  actualHours    Int? // Calculated from time tracking
  dueDate        DateTime?
  completedAt    DateTime?
  createdAt      DateTime           @default(now())
  updatedAt      DateTime           @updatedAt
  // Relations
  assignedTo     User?              @relation("SubProjectAssignee", fields: [assignedToId], references: [id])
  createdBy      User               @relation("SubProjectCreator", fields: [createdById], references: [id])
  qcHead         User?              @relation("SubProjectQcHead", fields: [qcHeadId], references: [id])
  project        Project            @relation(fields: [projectId], references: [id], onDelete: Cascade)
  // NEW: SubProject members (anyone in company can be added)
  members        SubProjectMember[]
  // NEW: Tasks within subproject
  tasks          Task[]
  timeTrackings  TimeTracking[]
  // Performance tracking
  totalTimeSpent Int                @default(0) // Minutes - denormalized for fast queries

  @@index([projectId])
  @@index([assignedToId])
  @@index([qcHeadId])
  @@index([status])
  @@map("sub_projects")
}

// NEW: SubProject members junction table
model SubProjectMember {
  id               String               @id @default(uuid())
  subProjectId     String
  userId           String
  role             SubProjectMemberRole @default(MEMBER)
  joinedAt         DateTime             @default(now())
  // Performance tracking per member per subproject
  tasksCompleted   Int                  @default(0)
  totalTimeMinutes Int                  @default(0)
  pointsEarned     Int                  @default(0)
  // Relations
  subProject       SubProject           @relation(fields: [subProjectId], references: [id], onDelete: Cascade)
  user             User                 @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([subProjectId, userId])
  @@index([subProjectId])
  @@index([userId])
  @@map("sub_project_members")
}

// NEW: Task assignees junction table (multi-assignee support)
model TaskAssignee {
  id           String    @id @default(uuid())
  taskId       String
  userId       String
  assignedAt   DateTime  @default(now())
  assignedById String?
  // Track individual completion status
  isCompleted  Boolean   @default(false)
  completedAt  DateTime?
  // Relations
  task         Task      @relation(fields: [taskId], references: [id], onDelete: Cascade)
  user         User      @relation("TaskAssignees", fields: [userId], references: [id], onDelete: Cascade)
  assignedBy   User?     @relation("TaskAssigner", fields: [assignedById], references: [id])

  @@unique([taskId, userId])
  @@index([taskId])
  @@index([userId])
  @@map("task_assignees")
}

// UPDATE: Task model
model Task {
  id               String     @id @default(uuid())
  title            String
  description      String?
  subProjectId     String
  // DEPRECATED: Keep for backward compatibility, use assignees relation instead
  assignedToId     String?
  createdById      String
  status           TaskStatus @default(TODO)
  priority         Priority   @default(MEDIUM)
  pointsValue      Int        @default(10)
  estimatedMinutes Int?
  actualMinutes    Int?
  dueDate          DateTime?
  startedAt        DateTime?
  completedAt      DateTime?
  createdAt        DateTime   @default(now())
  updatedAt        DateTime   @updatedAt

  // NEW: Review workflow fields
  submittedForReviewAt   DateTime?
  submittedForReviewById String?
  reviewedAt             DateTime?
  reviewedById           String?
  reviewStatus           ReviewStatus?
  reviewNotes            String? // QC feedback/rejection reason
  revisionCount          Int           @default(0)

  // NEW: Points deduction on rejection (optional)
  pointsDeducted Int @default(0)

  // Relations
  subProject           SubProject         @relation(fields: [subProjectId], references: [id], onDelete: Cascade)
  assignedTo           User?              @relation("TaskAssignee", fields: [assignedToId], references: [id])
  createdBy            User               @relation("TaskCreator", fields: [createdById], references: [id])
  submittedForReviewBy User?              @relation("TaskSubmitter", fields: [submittedForReviewById], references: [id])
  reviewedBy           User?              @relation("TaskReviewer", fields: [reviewedById], references: [id])
  timeTrackings        TaskTimeTracking[]
  // NEW: Multi-assignee relation
  assignees            TaskAssignee[]

  @@index([subProjectId])
  @@index([assignedToId])
  @@index([status])
  @@map("tasks")
}

// NEW: Task-level time tracking
model TaskTimeTracking {
  id              String    @id @default(uuid())
  taskId          String
  userId          String
  startTime       DateTime
  endTime         DateTime?
  durationMinutes Int       @default(0)
  notes           String?
  isActive        Boolean   @default(false)
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  task            Task      @relation(fields: [taskId], references: [id], onDelete: Cascade)

  @@index([taskId])
  @@index([userId])
  @@index([isActive])
  @@map("task_time_trackings")
}

model TimeTracking {
  id                    String       @id @default(uuid())
  userId                String
  subProjectId          String
  startTime             DateTime
  endTime               DateTime?
  durationMinutes       Int          @default(0)
  notes                 String?
  screenshots           String[] // Legacy field - keeping for compatibility
  isActive              Boolean      @default(false)
  createdAt             DateTime     @default(now())
  updatedAt             DateTime     @updatedAt
  subProject            SubProject   @relation(fields: [subProjectId], references: [id], onDelete: Cascade)
  user                  User         @relation(fields: [userId], references: [id], onDelete: Cascade)
  // New relation to Screenshot model
  screenCaptures        Screenshot[]
  // Track time deductions from deleted screenshots
  timeDeducted          Int          @default(0) // Total minutes deducted from deleted screenshots
  // Track if screen capture is required for this session
  screenCaptureRequired Boolean      @default(false)

  @@index([userId])
  @@index([subProjectId])
  @@index([isActive])
  @@map("time_trackings")
}

// NEW MODEL: Screenshot captures
model Screenshot {
  id              String        @id @default(uuid())
  timeTrackingId  String
  userId          String
  filePath        String // Path in Supabase storage
  fileUrl         String // Public URL
  fileSize        Int // Size in bytes
  capturedAt      DateTime // When the screenshot was taken
  uploadedAt      DateTime      @default(now())
  // Interval tracking for time deduction
  intervalStart   DateTime // Start of interval this screenshot represents
  intervalEnd     DateTime // End of interval (next screenshot's start or current time)
  intervalMinutes Int // Minutes this screenshot covers
  // Metadata
  screenWidth     Int?
  screenHeight    Int?
  monitorIndex    Int           @default(0) // Which monitor (0 = primary)
  // Verification
  checksum        String? // SHA-256 hash for tampering detection
  captureStatus   CaptureStatus @default(SUCCESS)
  // Soft delete for audit trail
  isDeleted       Boolean       @default(false)
  deletedAt       DateTime?
  deletedBy       String? // User who deleted (could be admin)
  deletionReason  String?
  // Auto-cleanup tracking
  expiresAt       DateTime // Auto-delete after 60 days
  timeTracking    TimeTracking  @relation(fields: [timeTrackingId], references: [id], onDelete: Cascade)

  @@index([timeTrackingId])
  @@index([userId])
  @@index([capturedAt])
  @@index([expiresAt])
  @@index([isDeleted])
  @@map("screenshots")
}

// NEW MODEL: Desktop Agent tracking
model DesktopAgent {
  id                 String    @id @default(uuid())
  userId             String
  machineId          String // Unique identifier for user's machine
  machineName        String? // Computer name
  platform           Platform // WINDOWS, MAC, LINUX
  agentVersion       String // Version of installed agent
  lastHeartbeat      DateTime? // Last time agent pinged server
  lastActiveAt       DateTime? // Last activity detected
  isOnline           Boolean   @default(false)
  isActive           Boolean   @default(true) // Can be disabled by admin
  installDate        DateTime  @default(now())
  updatedAt          DateTime  @updatedAt
  // Agent configuration
  captureQuality     Int       @default(70) // JPEG quality 1-100
  captureAllMonitors Boolean   @default(false)
  // Authentication
  agentToken         String    @unique // Token for agent API calls
  tokenExpiresAt     DateTime
  user               User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, machineId])
  @@index([userId])
  @@index([agentToken])
  @@index([isOnline])
  @@map("desktop_agents")
}

// NEW MODEL: Agent activity log for debugging/audit
model AgentActivityLog {
  id           String            @id @default(uuid())
  agentId      String
  activityType AgentActivityType
  description  String
  metadata     Json?
  createdAt    DateTime          @default(now())

  @@index([agentId])
  @@index([createdAt])
  @@map("agent_activity_logs")
}

// NEW: Leaderboard snapshot for historical tracking
model LeaderboardSnapshot {
  id                     String            @id @default(uuid())
  companyId              String
  userId                 String
  periodType             LeaderboardPeriod
  periodStart            DateTime
  periodEnd              DateTime
  rank                   Int
  // Metrics
  tasksCompleted         Int               @default(0)
  totalMinutes           Int               @default(0)
  pointsEarned           Int               @default(0)
  subProjectsContributed Int               @default(0)
  projectsContributed    Int               @default(0)
  // Performance score (weighted calculation)
  performanceScore       Float             @default(0)
  createdAt              DateTime          @default(now())

  @@index([companyId, periodType, periodStart])
  @@index([userId])
  @@index([rank])
  @@map("leaderboard_snapshots")
}

// NEW: Achievement/Badge system
model Achievement {
  id          String          @id @default(uuid())
  userId      String
  companyId   String
  type        AchievementType
  title       String
  description String
  iconUrl     String?
  earnedAt    DateTime        @default(now())
  metadata    Json? // Additional context

  @@index([userId])
  @@index([companyId, type])
  @@map("achievements")
}

model Sop {
  id              String    @id @default(uuid())
  title           String
  description     String?
  type            SopType
  fileUrl         String
  thumbnailUrl    String?
  duration        Int?
  status          SopStatus @default(PENDING_APPROVAL)
  companyId       String
  createdById     String
  approvedById    String?
  approvedAt      DateTime?
  rejectionReason String?
  viewCount       Int       @default(0)
  tags            String[]
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  approvedBy      User?     @relation("SopApprover", fields: [approvedById], references: [id])
  company         Company   @relation(fields: [companyId], references: [id], onDelete: Cascade)
  createdBy       User      @relation("SopCreator", fields: [createdById], references: [id])

  @@index([companyId])
  @@index([status])
  @@index([type])
  @@map("sops")
}

model ChatRoom {
  id          String           @id @default(uuid())
  name        String
  description String?
  projectId   String
  createdById String
  isActive    Boolean          @default(true)
  createdAt   DateTime         @default(now())
  updatedAt   DateTime         @updatedAt
  messages    ChatMessage[]
  members     ChatRoomMember[]
  createdBy   User             @relation(fields: [createdById], references: [id])
  project     Project          @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
  @@map("chat_rooms")
}

model ChatRoomMember {
  id         String   @id @default(uuid())
  chatRoomId String
  userId     String
  isQcAdmin  Boolean  @default(false)
  joinedAt   DateTime @default(now())
  chatRoom   ChatRoom @relation(fields: [chatRoomId], references: [id], onDelete: Cascade)
  user       User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([chatRoomId, userId])
  @@index([chatRoomId])
  @@index([userId])
  @@map("chat_room_members")
}

model ChatMessage {
  id         String   @id @default(uuid())
  chatRoomId String
  senderId   String
  content    String
  isEdited   Boolean  @default(false)
  isDeleted  Boolean  @default(false)
  createdAt  DateTime @default(now())
  chatRoom   ChatRoom @relation(fields: [chatRoomId], references: [id], onDelete: Cascade)
  sender     User     @relation(fields: [senderId], references: [id])

  @@index([chatRoomId])
  @@index([senderId])
  @@map("chat_messages")
}

model Notification {
  id        String           @id @default(uuid())
  userId    String
  type      NotificationType
  title     String
  message   String
  metadata  Json?
  isRead    Boolean          @default(false)
  createdAt DateTime         @default(now())
  user      User             @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId])
  @@index([isRead])
  @@map("notifications")
}

model ActivityLog {
  id           String       @id @default(uuid())
  companyId    String
  userId       String?
  activityType ActivityType
  description  String
  metadata     Json?
  ipAddress    String?
  createdAt    DateTime     @default(now())
  company      Company      @relation(fields: [companyId], references: [id], onDelete: Cascade)
  user         User?        @relation(fields: [userId], references: [id])

  @@index([companyId])
  @@index([userId])
  @@index([activityType])
  @@map("activity_logs")
}

// ============================================
// ENUMS
// ============================================

enum UserRole {
  USER
  QC_ADMIN
  COMPANY
  SUPER_ADMIN
}

enum SubscriptionStatus {
  TRIAL
  ACTIVE
  EXPIRED
  CANCELLED
}

enum ProjectStatus {
  PLANNING
  IN_PROGRESS
  ON_HOLD
  COMPLETED
  CANCELLED
}

enum ProjectMemberRole {
  MEMBER
  QC_ADMIN
  LEAD
}

enum SubProjectStatus {
  TODO
  IN_PROGRESS
  IN_REVIEW
  COMPLETED
}

// NEW: SubProject member roles
enum SubProjectMemberRole {
  MEMBER
  CONTRIBUTOR
  REVIEWER
  QC_HEAD
}

// NEW: Task status
enum TaskStatus {
  TODO
  IN_PROGRESS
  IN_REVIEW // Submitted for QC review
  NEEDS_REVISION // NEW: Rejected by QC, needs rework
  COMPLETED // Approved by QC
  BLOCKED
  CANCELLED
}

// NEW: Review status enum
enum ReviewStatus {
  PENDING
  APPROVED
  REJECTED
}

// NEW: Priority levels
enum Priority {
  LOW
  MEDIUM
  HIGH
  URGENT
  CRITICAL
}

enum SopType {
  VIDEO
  DOCUMENT
  PDF
  LINK
  IMAGE
}

enum SopStatus {
  DRAFT
  PENDING_APPROVAL
  APPROVED
  REJECTED
}

enum NotificationType {
  PROJECT_ASSIGNMENT
  TASK_ASSIGNMENT
  SOP_APPROVAL
  SOP_REJECTION
  CHAT_MESSAGE
  DEPARTMENT_ASSIGNMENT
  ROLE_CHANGE
  SYSTEM
  // Screen capture notification types
  SCREENSHOT_DELETED
  TIME_DEDUCTED
  AGENT_OFFLINE
  AGENT_INSTALLED
  // NEW: SubProject and Task notifications
  SUBPROJECT_ASSIGNMENT
  SUBPROJECT_MEMBER_ADDED
  SUBPROJECT_QC_HEAD_ASSIGNED
  TASK_CREATED
  TASK_COMPLETED
  TASK_REASSIGNED
  // NEW: Leaderboard and Achievement notifications
  LEADERBOARD_RANK_CHANGED
  ACHIEVEMENT_EARNED
  STREAK_MILESTONE

  TASK_SUBMITTED_FOR_REVIEW
  TASK_APPROVED
  TASK_REJECTED
  TASK_REVISION_REQUESTED
  POINTS_AWARDED
  POINTS_DEDUCTED
}

enum ActivityType {
  USER_LOGIN
  USER_LOGOUT
  PROJECT_CREATED
  PROJECT_UPDATED
  SOP_CREATED
  SOP_APPROVED
  TIME_TRACKING_START
  TIME_TRACKING_END
  DEPARTMENT_CREATED
  USER_ROLE_CHANGED
  // Screen capture activity types
  SCREENSHOT_CAPTURED
  SCREENSHOT_DELETED
  AGENT_REGISTERED
  AGENT_DISCONNECTED
  // NEW: SubProject and Task activity types
  SUBPROJECT_CREATED
  SUBPROJECT_UPDATED
  SUBPROJECT_MEMBER_ADDED
  SUBPROJECT_MEMBER_REMOVED
  TASK_CREATED
  TASK_ASSIGNED
  TASK_COMPLETED
  TASK_DELETED
  // NEW: Leaderboard activity types
  LEADERBOARD_UPDATED
  ACHIEVEMENT_UNLOCKED
}

// NEW ENUMS for screen capture
enum CaptureStatus {
  SUCCESS
  FAILED
  PERMISSION_DENIED
  IDLE_DETECTED
  SCREEN_LOCKED
  OFFLINE
}

enum Platform {
  WINDOWS
  MAC
  LINUX
}

enum AgentActivityType {
  STARTED
  STOPPED
  HEARTBEAT
  CAPTURE_SUCCESS
  CAPTURE_FAILED
  ERROR
  UPDATED
  PERMISSION_CHANGED
}

// NEW: Leaderboard period types
enum LeaderboardPeriod {
  DAILY
  WEEKLY
  MONTHLY
  QUARTERLY
  YEARLY
  ALL_TIME
}

