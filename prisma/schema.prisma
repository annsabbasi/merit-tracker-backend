  HOURS_100_TRACKED
  HOURS_500_TRACKED
  HOURS_1000_TRACKED
  // Streak achievements
  STREAK_7_DAYS
  STREAK_30_DAYS
  STREAK_90_DAYS
  STREAK_365_DAYS
  // Performance achievements
  TOP_PERFORMER_WEEK
  TOP_PERFORMER_MONTH
  MOST_IMPROVED
  // Collaboration achievements
  TEAM_PLAYER
  MENTOR
  EARLY_BIRD
  NIGHT_OWL
  // Quality achievements
  QUALITY_CHAMPION
  ZERO_DEFECTS
  // Custom achievements
  CUSTOM
}
